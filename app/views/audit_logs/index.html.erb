<h1>Audit Log Dashboard</h1>

<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th align="center">
        #
      </th>
      <th align="center">
        Employee
      </th>
      <th align="center">
        Week Starting
      </th>
      <th align="center">
        Confirmed
      </th>
      <th align="center">
        Status
      </th>
    </tr>
  </thead>
  <tbody id="audit_logs">
    <%= render @audit_logs %>
  </tbody>
</table>

<div id="paginator">
<%= paginate @audit_logs, window: 1, remote: true %>
</div>